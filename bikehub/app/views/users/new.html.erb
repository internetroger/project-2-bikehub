<h1>New User Signup</h1>

<%= form_for @user do |f| %>
    <%=f.label "Username:" %>
    <%= f.text_field :username %><br>
    <%=f.label "Profile"%>
    <%= f.text_area :profile %>
    <%= f.label "Password:" %>
    <%= f.password_field :password %>
    <%= f.password_field :password_confirmation %>
    <%=f.submit %>
<% end %>
